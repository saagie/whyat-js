{
  "name": "whyat-js",
  "version": "0.1.28",
  "description": "Y@ (whyat) helps you to track your userâ€™s action within your application and store it directly in the data source of your choice Edit",
  "main": "lib/cjs/tracker.js",
  "module": "lib/es/tracker.js",
  "files": [
    "dist",
    "lib",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register test/",
    "build:cjs": "babel src -d lib/cjs",
    "build:es": "gulp build:es",
    "build:umd": "browserify src/tracker.js -t [ babelify --presets [ env ] ] -o dist/whyat.js",
    "build:umd:min": "browserify src/tracker.js -t [ babelify --presets [ env ] ] | uglifyjs -c -m -o dist/whyat.min.js",
    "build": "npm run build:es && npm run build:cjs",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saagie/whyat-js.git"
  },
  "keywords": [
    "tracker",
    "event"
  ],
  "author": "Saagie Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/saagie/whyat-js/issues"
  },
  "homepage": "https://github.com/saagie/whyat-js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "eslint": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^3.4.2",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0",
    "uglify": "^0.1.5",
    "whatwg-fetch": "^2.0.3"
  },
  "peerDependencies": {
    "babel-polyfill": "^6.23.0",
    "whatwg-fetch": "^2.0.3"
  }
}
