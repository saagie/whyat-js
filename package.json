{
  "name": "whyat-js",
  "version": "0.4.0",
  "description": "Y@ (whyat) helps you to track your userâ€™s action within your application and store it directly in the data source of your choice Edit",
  "main": "dist/whyat.min.js",
  "files": [
    "dist",
    "lib",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "test": "mocha --require babel-polyfill spec/setup.js --compilers js:babel-register test/",
    "test:watch": "npm run test -- -w",
    "build:cjs": "babel src -d lib/cjs",
    "build:es": "gulp build:es",
    "build:umd": "browserify src/tracker.js -x 'tcomb' -x 'whatwg-fetch' -t [ babelify --presets [ env ] ] --standalone whyat-js -o dist/whyat.js",
    "build:umd:min": "npm run build:umd && uglifyjs dist/whyat.js -o dist/whyat.min.js",
    "build": "npm run build:es && npm run build:cjs && npm run build:umd:min",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saagie/whyat-js.git"
  },
  "keywords": [
    "tracker",
    "event"
  ],
  "author": "Saagie Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/saagie/whyat-js/issues"
  },
  "homepage": "https://github.com/saagie/whyat-js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.0.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0",
    "uglify": "^0.1.5"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.3",
    "tcomb": "^3.2.21"
  }
}
